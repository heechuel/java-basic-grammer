import java.util.Scanner;


public class Main
{
	public static void main(String[] args) {
	    
	   Scanner scan = new Scanner(System.in);
	   
	   int[] arry;
	   arry = new int[5];
	   int max = 0;
	   
	   System.out.print(" 5개의 양수를 입력하시오 : ");
	   
	   for(int i =0;i<5;i++)
	   {
	       arry[i] = scan.nextInt();
	       if(max < arry[i])
	            max = arry[i];
	   }
	   
	   System.out.println("가장 큰 수는 : " + max);
	   
	   scan.close();
	}
}
